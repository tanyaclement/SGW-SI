    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "l-e-sissman-at-sir-george-williams-university-the-poetry-series-7-april-1972-canvas-1-l-e--sissman-annotation-0.json",
      "type": "Annotation",
