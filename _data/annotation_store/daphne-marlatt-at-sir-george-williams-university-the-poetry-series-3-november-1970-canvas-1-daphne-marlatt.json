    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "daphne-marlatt-at-sir-george-williams-university-the-poetry-series-3-november-1970-canvas-1-daphne-marlatt-annotation-1.json",
      "type": "Annotation",
